{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "start:terminal",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "args": [
        "--enable-source-maps",
        "${workspaceFolder}/open-chatbot-js/dist/src/index.js",
        "terminal"
      ],
      "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
        "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome-stable"
      },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "outputCapture": "std",
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "restart": false,
      "sourceMapPathOverrides": {
        "webpack:///./~/*": "${webRoot}/node_modules/*",
        "webpack:///./src/*": "${webRoot}/*"
      }
    },
    {
      "name": "start:sttts",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "args": [
        "--enable-source-maps",
        "${workspaceFolder}/open-chatbot-js/dist/src/index.js",
        "sttts"
      ],
      "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
        "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome-stable"
      },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "outputCapture": "std",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "restart": false,
      "sourceMapPathOverrides": {
        "webpack:///./~/*": "${webRoot}/node_modules/*",
        "webpack:///./src/*": "${webRoot}/*"
      }
    },
    {
      "name": "start:discord",
      "type": "node",
      "request": "launch",
      "protocol": "inspector",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "node",
      "args": [
        "--enable-source-maps",
        "${workspaceFolder}/open-chatbot-js/dist/src/index.js",
        "discord"
      ],
      "env": {
        "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD": "true",
        "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome-stable"
      },
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "outputCapture": "std",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"],
      "restart": false,
      "sourceMapPathOverrides": {
        "webpack:///./~/*": "${webRoot}/node_modules/*",
        "webpack:///./src/*": "${webRoot}/*"
      }
    },
    {
      "name": "Launch gpt4all",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/gpt4all-rest/src/gpt4all-rest.py",
      "console": "integratedTerminal",
      "justMyCode": false
    }
  ]
}
